{
    "name": "giansalex/sufel-client",
    "description": "Sufel Php Client for Company",
    "keywords": [
        "sufel",
        "php",
        "sdk",
        "api"
    ],
    "homepage": "https://github.com/giansalex/sufel-php-client",
    "license": "MIT",
    "authors": [
        {
            "name": "Giancarlos Salas",
            "email": "giansalex@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^6.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8"
    },
    "autoload": {
        "psr-4": { "Sufel\\Client\\" : "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Sufel\\Client\\" : "test/" }
    }
}
